---
import SEO from '../../components/SEO.astro';
import Layout from '../../layouts/Layout.astro';
import AmazonSeo from '../../react_pages/landing/AmazonSeo.tsx';

const title = "Otimize Suas Listagens com Amazon SEO";
const description = "Aprenda como funciona o algoritmo A9 da Amazon e solicite uma auditoria gratuita para melhorar o ranking dos seus produtos.";
---

<Layout title={title}>
    <SEO
        title={title}
        description={description}
    />
    <AmazonSeo client:load />
</Layout>