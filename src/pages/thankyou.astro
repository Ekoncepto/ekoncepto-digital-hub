---
import Layout from '../layouts/Layout.astro';
import ThankYou from '../react_pages/ThankYou.tsx';
import SEO from '../components/SEO.astro';

const title = "Obrigado pelo Contato!";
const description = "Agradecemos seu contato. Entraremos em contato em breve.";
---

<Layout title={title}>
    <SEO
        title={title}
        description={description}
        noIndex
    />
    <ThankYou client:load />
</Layout>
