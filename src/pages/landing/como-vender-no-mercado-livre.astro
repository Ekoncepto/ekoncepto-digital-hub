---
import SEO from '../../components/SEO.astro';
import Layout from '../../layouts/Layout.astro';
import ComoVenderNoMercadoLivre from '../../react_pages/landing/ComoVenderNoMercadoLivre.tsx';

const title = "Guia Definitivo: Como Vender no Mercado Livre e Lucrar Mais";
const description = "Nosso guia completo ensina o passo a passo para iniciantes que desejam ter sucesso no Mercado Livre.";
