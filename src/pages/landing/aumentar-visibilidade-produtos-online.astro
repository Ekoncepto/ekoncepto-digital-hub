---
import SEO from '../../components/SEO.astro';
import Layout from '../../layouts/Layout.astro';
import AumentarVisibilidadeProdutosOnline from '../../react_pages/landing/AumentarVisibilidadeProdutosOnline.tsx';

const title = "Torne Seus Produtos Mais Visíveis Online";
const description = "Receba um checklist com técnicas de SEO e Ads para aumentar a visibilidade dos seus produtos e atrair mais clientes.";
---

<Layout title={title}>
    <SEO
        title={title}
        description={description}
    />
    <AumentarVisibilidadeProdutosOnline client:load />
</Layout>