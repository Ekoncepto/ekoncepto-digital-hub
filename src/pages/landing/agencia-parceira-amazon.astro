---
import SEO from '../../components/SEO.astro';
import Layout from '../../layouts/Layout.astro';
import AgenciaParceiraAmazon from '../../react_pages/landing/AgenciaParceiraAmazon.tsx';

const title = "Parceiro Certificado para Suas Vendas na Amazon";
const description = "Somos uma agência parceira certificada pela Amazon. Conheça nossos cases de sucesso e veja como podemos escalar suas vendas.";
---

