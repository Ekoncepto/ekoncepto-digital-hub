---
import SEO from '../../components/SEO.astro';
import Layout from '../../layouts/Layout.astro';
import CriacaoAplusContentAmazon from '../../react_pages/landing/CriacaoAplusContentAmazon.tsx';

const title = "Seu Produto com Apresentação A+ de Alta Conversão na Amazon";
const description = "Descubra como o conteúdo A+ pode transformar a página do seu produto e veja nosso portfólio.";
