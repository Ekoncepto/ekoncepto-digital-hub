---
import SEO from '../../components/SEO.astro';
import Layout from '../../layouts/Layout.astro';
import GestaoDeAnunciosEmMarketplaces from '../../react_pages/landing/GestaoDeAnunciosEmMarketplaces.tsx';

const title = "Anúncios que Convertem em Qualquer Marketplace";
const description = "Com nossa expertise multicanal, criamos e otimizamos anúncios para gerar o máximo de resultado em qualquer plataforma.";
---

<Layout title={title}>
    <SEO
        title={title}
        description={description}
    />
    <GestaoDeAnunciosEmMarketplaces client:load />
</Layout>